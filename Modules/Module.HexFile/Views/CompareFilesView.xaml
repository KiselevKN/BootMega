<theme:DialogWindow x:Class="Module.HexFile.Views.CompareFilesView"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:theme="clr-namespace:BootMega.Theme;assembly=BootMega.Theme"
                    Icon="{StaticResource CompareFilesWhiteIcon}"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:r="clr-namespace:Module.HexFile.Properties"
                    Title="Сравнение файлов" 
                    SizeToContent="WidthAndHeight">
    <theme:DialogWindow.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/BootMega.Theme;component/Themes/Generic.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Module.HexFile;component/Themes/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </theme:DialogWindow.Resources>
    <ContentPresenter Content="{Binding}"/>
    </theme:DialogWindow>
